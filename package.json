{
  "name": "rudex",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:42VN-Pasila/rudex.git",
  "author": "Kha Tieu",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/server.ts",
    "clean": "rm -rf ./dist/ ./src/gen/",
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint './**/*.ts' --quiet",
    "test": "jest --config ./jest.config.js --forceExit",
    "test:e2e": "jest --config ./jest-e2e.config.js --runInBand --forceExit",
    "gen": "yarn gen:openapi && yarn gen:prisma",
    "gen:openapi": "openapi-typescript docs/openapi.yml --output src/gen/server.d.ts",
    "gen:prisma": "prisma generate"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@prisma/client": "^6.18.0",
    "@types/bull": "^4.10.4",
    "bull": "^4.16.5",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "prisma": "^6.18.0",
    "sqlite3": "^5.1.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.25.0",
    "@types/node": "^22.9.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
